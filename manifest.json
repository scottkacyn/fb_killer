{
  "manifest_version": 2,

  "name": "Facebook Killer Extension",
  "description": "Remove annoying shit from Facebook",
  "version": "1.0",
  "content_scripts": [ {
    "js": [ "jquery-2.1.3.min.js", "content.js" ],
    "matches": ["http://www.facebook.com/*", "https://www.facebook.com/*"]
  }],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://*/*",
    "https://*/*",
    "http://localhost/"
  ]
}